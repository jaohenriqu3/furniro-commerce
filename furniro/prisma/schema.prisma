generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                     String @id @default(uuid())
  name                   String
  add_on_addres          String
  additional_information String
  city                   String
  company_name           String
  county                 String
  email_addres           String
  last_name              String
  province               String
  street_addres          String
  zip_code               String
  password               String
}

model Product {
  id          String @id @default(uuid())
  name        String
  price       String
  description String
  category    String
  size        String
  sku         String
  tag         String
  imageURL    String
}
